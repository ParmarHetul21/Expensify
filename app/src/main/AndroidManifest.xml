        <activity android:name=".Budget" />
        <activity android:name=".MonthView" />
        <activity android:name=".CustomView" />
        <activity android:name=".DayView" />
        <activity android:name=".addincome" />
        <activity android:name=".AllTransactions" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

